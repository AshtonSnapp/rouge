func factorial(nat n) nat do:
	if n == 1 then:
		return 1
	else:
		return n * factorial(n - 1)
	end
end

pub func main() ubyte do:
	if uint::from_str(prompt!("Give me a positive whole number: ")) is:
		Ok(num) then:
			outl!("{}! = {}", num, factorial(num))
			return 0
		end
		Err(_) then:
			errl!("{} isn't a positive whole number!", num)
			return 1
		end
	end
end