func factorial(n: nat) -> nat do
	if n == 1 then
		1
	else
		n * factorial(n - 1)
	end
end

pub func main() do
	if nat::from_str(promptln("Give me a positive whole number: ")) matches
		Ok(num) then outln("\{num}! = \{factorial(num)}")
		Err(_) then errln("That isn't a positive whole number!")
	end
end