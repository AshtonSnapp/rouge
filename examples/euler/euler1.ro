# https://projecteuler.net/problem=1

func sumMultiplesBelow(factors: [nat], max: nat) -> nat do
	mut sum := 0

	for i in 0..=max do
		for factor in factors do
			if i %% factor then
				sum += i
				break
			end
		end
	end

	sum
end

pub func main() do
	outln("\{sumMultiplesBelow([3, 5], 1000)}")
end