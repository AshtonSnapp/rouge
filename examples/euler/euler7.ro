# https://projecteuler.net/problem=7

func xPrimes(x: nat): [nat] do
	mut primes := [2]
	mut y: nat = 2

	`outer loop
		y += 1
		for prime in primes do
			if y % prime == 0
				continue outer
			end
		end
		primes.push_back(y)
		if primes.len() == x + 1 then
			break
		end
	end

	primes
end

pub func main() do
	outl("\{xPrimes(10001)[-1]}")
end